{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$defs": {
		"iso8601": {
			"type": "string",
			"description": "Similar to the standard date type, but each section after the year is optional. e.g. 2014-06-29 or 2023-04",
			"pattern": "^([1-2][0-9]{3}-[0-1][0-9]-[0-3][0-9]|[1-2][0-9]{3}-[0-1][0-9]|[1-2][0-9]{3})$"
		},
		"Email": {
			"type": "string",
			"description": "Email address",
			"title": "email",
			"format": "email",
			"examples": ["<local-part>@<domain>", "foo@bar.com", "foo.bar@baz.com"]
		},
		"UrlString": {
			"description": "URL string that fulfills the URL requirements (as per RFC 3986)",
			"examples": [
				"https://example.com",
				"https://example.com/#section",
				"http://example.com:8080",
				"http://üçåüçåüçå.ws",
				"https://www.√ºbercool.de"
			],
			"format": "uri",
			"title": "url",
			"type": "string"
		},
		"NonEmptyString": {
			"type": "string",
			"description": "a non empty string",
			"title": "non empty string",
			"minLength": 1,
			"examples": ["' test string  '", "'test string'"]
		}
	},
	"additionalProperties": false,
	"properties": {
		"$schema": {
			"description": "link to the version of the schema that can validate the resume",
			"examples": ["http://jsonresume.org/schema"],
			"format": "uri",
			"title": "$schema",
			"type": "string"
		},
		"awards": {
			"additionalItems": false,
			"description": "Specify any awards you have received throughout your professional career",
			"items": {
				"additionalProperties": false,
				"properties": {
					"awarder": {
						"description": "The name of the award given",
						"examples": ["Time Magazine"],
						"title": "awarder",
						"type": "string"
					},
					"date": {
						"$ref": "#/$defs/iso8601"
					},
					"summary": {
						"description": "A brief summary of the award",
						"examples": ["Received for my work with Quantum Physics"],
						"title": "summary",
						"type": "string"
					},
					"title": {
						"description": "Title of the award",
						"examples": ["One of the 100 greatest minds of the century"],
						"title": "title",
						"type": "string"
					}
				},
				"required": [],
				"type": "object"
			},
			"type": "array"
		},
		"basics": {
			"additionalProperties": false,
			"properties": {
				"email": {
					"$ref": "#/$defs/Email"
				},
				"image": {
					"$ref": "#/$defs/UrlString",
					"title": "image"
				},
				"label": {
					"description": "Label",
					"examples": ["Web Developer"],
					"title": "label",
					"type": "string"
				},
				"location": {
					"additionalProperties": false,
					"properties": {
						"address": {
							"description": "To add multiple address lines, use \"\\n\".",
							"examples": ["1234 Gl√ºcklichkeit Stra√üe\nHinterhaus 5. Etage li."],
							"title": "address",
							"type": "string"
						},
						"city": {
							"description": "City",
							"examples": ["Berlin", "New York", "San Francisco"],
							"title": "city",
							"type": "string"
						},
						"countryCode": {
							"description": "code as per ISO-3166-1 ALPHA-2, e.g. US, AU, IN",
							"title": "countryCode",
							"type": "string"
						},
						"postalCode": {
							"description": "European postal code",
							"examples": ["12209"],
							"title": "postalCode",
							"type": "string"
						},
						"region": {
							"description": "The general region where you live. Can be a US state, or a province",
							"examples": ["California", "Quebec"],
							"title": "region",
							"type": "string"
						}
					},
					"required": ["city", "countryCode"],
					"type": "object"
				},
				"name": {
					"description": "Your full name",
					"examples": ["Thomas Anderson"],
					"title": "name",
					"type": "string"
				},
				"phone": {
					"description": "Phone numbers are stored as strings so use any format you like, e.g. 712-117-2923",
					"type": "string"
				},
				"profiles": {
					"additionalItems": false,
					"description": "Specify any number of social networks that you participate in",
					"items": {
						"additionalProperties": false,
						"properties": {
							"network": {
								"description": "The name of the social network",
								"examples": ["Facebook", "Twitter"],
								"title": "network",
								"type": "string"
							},
							"url": {
								"$ref": "#/$defs/UrlString",
								"description": "e.g. http://twitter.example.com/neutralthoughts"
							},
							"username": {
								"description": "The username of the profile on the social network",
								"examples": ["neutralthoughts"],
								"title": "username",
								"type": "string"
							}
						},
						"required": ["network", "url", "username"],
						"type": "object"
					},
					"type": "array"
				},
				"summary": {
					"description": "Write a short 2-3 sentence biography about yourself",
					"examples": ["Web Developer with a passion for web-based applications"],
					"title": "summary",
					"type": "string"
				},
				"url": {
					"$ref": "#/$defs/UrlString",
					"description": "URL (as per RFC 3986) to your website, e.g. personal homepage"
				}
			},
			"required": ["name", "label", "email", "summary", "location", "profiles"],
			"type": "object"
		},
		"certificates": {
			"additionalItems": false,
			"description": "Specify any certificates you have received throughout your professional career",
			"items": {
				"additionalProperties": false,
				"properties": {
					"date": {
						"description": "a string",
						"title": "string",
						"type": "string"
					},
					"issuer": {
						"description": "issuer of the certificate",
						"examples": ["e.g. CNCF"],
						"title": "issuer",
						"type": "string"
					},
					"name": {
						"description": "name of the certificate",
						"examples": ["e.g. Certified Kubernetes Administrator"],
						"title": "Name",
						"type": "string"
					},
					"url": {
						"$ref": "#/$defs/UrlString"
					}
				},
				"required": [],
				"type": "object"
			},
			"type": "array"
		},
		"education": {
			"additionalItems": false,
			"items": {
				"additionalProperties": true,
				"properties": {
					"area": {
						"description": "e.g. Arts",
						"examples": ["Arts", "Computer Science"],
						"title": "area",
						"type": "string"
					},
					"courses": {
						"additionalItems": false,
						"description": "List notable courses/subjects",
						"items": {
							"description": "a string",
							"examples": ["H1302 - Introduction to American history"],
							"title": "course",
							"type": "string"
						},
						"type": "array"
					},
					"endDate": {
						"$ref": "#/$defs/iso8601"
					},
					"institution": {
						"description": "name of the institution",
						"examples": ["Massachusetts Institute of Technology"],
						"title": "institution",
						"type": "string"
					},
					"location": {
						"$ref": "#/$defs/NonEmptyString"
					},
					"score": {
						"description": "grade point average, e.g. 3.67/4.0",
						"examples": ["3.67/4.0"],
						"title": "score",
						"type": "string"
					},
					"startDate": {
						"$ref": "#/$defs/iso8601"
					},
					"studyType": {
						"description": "the type of study",
						"examples": ["Bachelor", "Master", "Doctorate"],
						"title": "studyType",
						"type": "string"
					},
					"url": {
						"$ref": "#/$defs/UrlString"
					}
				},
				"required": ["area", "institution", "startDate", "studyType"],
				"type": "object"
			},
			"type": "array"
		},
		"interests": {
			"additionalItems": false,
			"items": {
				"additionalProperties": false,
				"properties": {
					"keywords": {
						"items": {
							"description": "a string",
							"examples": ["philosophy"],
							"title": "keyword",
							"type": "string"
						},
						"type": "array"
					},
					"name": {
						"description": "Interest name",
						"examples": ["Philosophy"],
						"title": "name",
						"type": "string"
					}
				},
				"required": [],
				"type": "object"
			},
			"type": "array"
		},
		"languages": {
			"additionalItems": false,
			"description": "List any other languages you speak",
			"items": {
				"additionalProperties": false,
				"properties": {
					"fluency": {
						"description": "e.g. English, Spanish",
						"examples": ["English", "Spanish"],
						"title": "language",
						"type": "string"
					},
					"language": {
						"description": "e.g. English, Spanish",
						"type": "string"
					}
				},
				"required": [],
				"type": "object"
			},
			"type": "array"
		},
		"projects": {
			"additionalItems": false,
			"description": "Specify career projects",
			"items": {
				"additionalProperties": false,
				"properties": {
					"description": {
						"description": "Short summary of project",
						"examples": ["Collated works of 2017"],
						"title": "description",
						"type": "string"
					},
					"endDate": {
						"$ref": "#/$defs/iso8601"
					},
					"entity": {
						"description": "Specify the relevant company/entity affiliations",
						"examples": ["Greenpeace", "Microsoft"],
						"title": "entity",
						"type": "string"
					},
					"highlights": {
						"additionalItems": false,
						"description": "Specify multiple features",
						"items": {
							"description": "Specify multiple features",
							"examples": ["Directs you close but not quite there"],
							"title": "highlight",
							"type": "string"
						},
						"type": "array"
					},
					"keywords": {
						"additionalItems": false,
						"description": "Specify special elements involved",
						"items": {
							"description": "a string",
							"examples": ["AngularJS", "elements"],
							"title": "keyword",
							"type": "string"
						},
						"type": "array"
					},
					"name": {
						"description": "Name of the project",
						"examples": ["The World Wide Web"],
						"title": "name",
						"type": "string"
					},
					"roles": {
						"additionalItems": false,
						"description": "Specify your role on this project or in company",
						"items": {
							"description": "a string",
							"examples": ["Team Lead", "Speaker", "Writer"],
							"title": "role",
							"type": "string"
						},
						"type": "array"
					},
					"startDate": {
						"$ref": "#/$defs/iso8601"
					},
					"type": {
						"description": "Type of project",
						"examples": ["volunteering", "presentation", "talk", "application", "conference"],
						"title": "type",
						"type": "string"
					},
					"url": {
						"$ref": "#/$defs/UrlString"
					}
				},
				"required": [],
				"type": "object"
			},
			"type": "array"
		},
		"publications": {
			"additionalItems": false,
			"description": "Specify your publications through your career",
			"items": {
				"additionalProperties": false,
				"properties": {
					"name": {
						"description": "The name of the publication",
						"examples": ["The World Wide Web"],
						"title": "name",
						"type": "string"
					},
					"publisher": {
						"description": "The publisher of the publication",
						"examples": ["IEEE", "Computer Magazine"],
						"title": "publisher",
						"type": "string"
					},
					"releaseDate": {
						"$ref": "#/$defs/iso8601"
					},
					"summary": {
						"description": "Short summary of publication",
						"examples": ["Discussion of the World Wide Web, HTTP, HTML"],
						"title": "summary",
						"type": "string"
					},
					"url": {
						"$ref": "#/$defs/UrlString",
						"examples": [
							"e.g. http://www.computer.org.example.com/csdl/mags/co/1996/10/rx069-abs.html"
						]
					}
				},
				"type": "object"
			},
			"type": "array"
		},
		"references": {
			"additionalItems": false,
			"description": "List references you have received",
			"items": {
				"additionalProperties": false,
				"properties": {
					"name": {
						"description": "The name of the reference",
						"examples": ["Timothy Cook"],
						"title": "name",
						"type": "string"
					},
					"reference": {
						"description": "The reference text",
						"examples": [
							"Joe blogs was a great employee, who turned up to work at least once a week. He exceeded my expectations when it came to doing nothing."
						],
						"title": "reference",
						"type": "string"
					}
				},
				"required": ["name"],
				"type": "object"
			},
			"type": "array"
		},
		"skills": {
			"additionalItems": false,
			"description": "List out your professional skill-set",
			"items": {
				"additionalProperties": false,
				"properties": {
					"keywords": {
						"additionalItems": false,
						"description": "List some keywords pertaining to this skill",
						"items": {
							"description": "a string",
							"examples": ["Rust"],
							"title": "keyword",
							"type": "string"
						},
						"type": "array"
					},
					"level": {
						"type": "string",
						"description": "Level of expertise",
						"title": "level",
						"examples": ["Master", "Intermediate"]
					},
					"name": {
						"description": "Name of the skill",
						"examples": ["Web Development"],
						"title": "name",
						"type": "string"
					}
				},
				"required": ["name", "keywords"],
				"type": "object"
			},
			"type": "array"
		},
		"volunteer": {
			"additionalItems": false,
			"items": {
				"additionalProperties": false,
				"properties": {
					"endDate": {
						"$ref": "#/$defs/iso8601"
					},
					"highlights": {
						"additionalItems": false,
						"description": "Specify accomplishments and achievements",
						"items": {
							"description": "a string",
							"examples": ["Increased profits by 20% from 2011-2012 through viral advertising"],
							"title": "highlight",
							"type": "string"
						},
						"type": "array"
					},
					"organization": {
						"description": "Name of the organization",
						"examples": ["Facebook"],
						"title": "organization",
						"type": "string"
					},
					"position": {
						"description": "The title of your position at the company",
						"examples": ["Software Engineer"],
						"title": "position",
						"type": "string"
					},
					"startDate": {
						"$ref": "#/$defs/iso8601"
					},
					"summary": {
						"description": "Give an overview of your responsibilities at the company",
						"examples": [
							"My day-to-day activities involved designing and building web applications..."
						],
						"title": "summary",
						"type": "string"
					},
					"url": {
						"$ref": "#/$defs/UrlString"
					}
				},
				"required": [],
				"type": "object"
			},
			"type": "array"
		},
		"work": {
			"additionalItems": false,
			"items": {
				"additionalProperties": false,
				"properties": {
					"contact": {
						"additionalProperties": false,
						"properties": {
							"email": {
								"$ref": "#/$defs/Email"
							},
							"name": {
								"description": "The name and role of the contact person",
								"examples": ["Mark Zuckerberg (CTO)"],
								"title": "name",
								"type": "string"
							},
							"phone": {
								"description": "a string",
								"title": "string",
								"type": "string"
							}
						},
						"required": ["name", "email"],
						"type": "object"
					},
					"description": {
						"description": "A short description of the company",
						"examples": ["Social Media Company", "Educational Software Company"],
						"title": "description",
						"type": "string"
					},
					"endDate": {
						"$ref": "#/$defs/iso8601"
					},
					"highlights": {
						"additionalItems": false,
						"description": "Specify multiple accomplishments",
						"items": {
							"description": "e.g. Increased profits by 20% from 2011-2012 through viral advertising",
							"type": "string"
						},
						"type": "array"
					},
					"location": {
						"description": "Location of the company",
						"examples": ["Menlo Park, CA"],
						"title": "location",
						"type": "string"
					},
					"name": {
						"description": "Name of the company",
						"examples": ["Facebook"],
						"title": "name",
						"type": "string"
					},
					"roles": {
						"additionalItems": {
							"additionalProperties": false,
							"properties": {
								"endDate": {
									"type": "string",
									"description": "a string",
									"title": "string"
								},
								"highlights": {
									"additionalItems": {
										"description": "A specific accomplishment",
										"examples": [
											"Increased profits by 20% from 2011-2012 through viral advertising"
										],
										"title": "highlight",
										"type": "string"
									},
									"items": [
										{
											"description": "A specific accomplishment",
											"examples": [
												"Increased profits by 20% from 2011-2012 through viral advertising"
											],
											"title": "highlight",
											"type": "string"
										}
									],
									"minItems": 1,
									"type": "array"
								},
								"responsibilities": {
									"items": {
										"description": "A specific responsibility",
										"examples": ["code owner of x"],
										"title": "responsibility",
										"type": "string"
									},
									"type": "array"
								},
								"startDate": {
									"description": "a string",
									"title": "string",
									"type": "string"
								},
								"technologies": {
									"additionalItems": {
										"description": "A specific technology used",
										"examples": ["React", "Node.js"],
										"title": "technology",
										"type": "string"
									},
									"items": [
										{
											"description": "A specific technology used",
											"examples": ["React", "Node.js"],
											"title": "technology",
											"type": "string"
										}
									],
									"minItems": 1,
									"type": "array"
								},
								"title": {
									"description": "The title of your position at the company: <seniority level> <role title>",
									"examples": ["Junior Software Developer", "Senior Software Engineer"],
									"title": "title",
									"type": "string"
								}
							},
							"required": ["title", "startDate", "responsibilities"],
							"type": "object"
						},
						"items": [
							{
								"additionalProperties": false,
								"properties": {
									"endDate": {
										"description": "a string",
										"title": "string",
										"type": "string"
									},
									"highlights": {
										"additionalItems": {
											"description": "A specific accomplishment",
											"examples": [
												"Increased profits by 20% from 2011-2012 through viral advertising"
											],
											"title": "highlight",
											"type": "string"
										},
										"items": [
											{
												"description": "A specific accomplishment",
												"examples": [
													"Increased profits by 20% from 2011-2012 through viral advertising"
												],
												"title": "highlight",
												"type": "string"
											}
										],
										"minItems": 1,
										"type": "array"
									},
									"responsibilities": {
										"type": "array",
										"items": {
											"description": "A specific responsibility",
											"examples": ["code owner of x"],
											"title": "responsibility",
											"type": "string"
										}
									},
									"startDate": {
										"description": "a string",
										"title": "string",
										"type": "string"
									},
									"technologies": {
										"additionalItems": {
											"description": "A specific technology used",
											"examples": ["React", "Node.js"],
											"title": "technology",
											"type": "string"
										},
										"items": [
											{
												"description": "A specific technology used",
												"examples": ["React", "Node.js"],
												"title": "technology",
												"type": "string"
											}
										],
										"minItems": 1,
										"type": "array"
									},
									"title": {
										"description": "The title of your position at the company: <seniority level> <role title>",
										"examples": ["Junior Software Developer", "Senior Software Engineer"],
										"title": "title",
										"type": "string"
									}
								},
								"required": ["title", "startDate", "responsibilities"],
								"type": "object"
							}
						],
						"minItems": 1,
						"type": "array"
					},
					"startDate": {
						"$ref": "#/$defs/iso8601"
					},
					"summary": {
						"description": "A brief introduction of what the company does; a tagline, a mission statement, an elevator pitch; something that gives a sense of the company in a few words",
						"examples": [
							"An educational software company that helps students learn through interactive games",
							"Help users to handle all their administrative tasks digitally with ease."
						],
						"title": "summary",
						"type": "string"
					},
					"techStack": {
						"additionalItems": {
							"$ref": "#/$defs/NonEmptyString"
						},
						"items": [
							{
								"$ref": "#/$defs/NonEmptyString"
							}
						],
						"minItems": 1,
						"type": "array"
					},
					"url": {
						"$ref": "#/$defs/UrlString",
						"description": "e.g. http://facebook.example.com"
					}
				},
				"required": ["description", "name", "startDate", "roles"],
				"type": "object"
			},
			"type": "array"
		}
	},
	"required": ["$schema", "basics", "education", "skills", "work"],
	"title": "Resume Schema",
	"type": "object"
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "$schema",
    "basics",
    "education",
    "skills",
    "work"
  ],
  "properties": {
    "$schema": {
      "type": "string",
      "description": "link to the version of the schema that can validate the resume",
      "title": "$schema",
      "examples": [
        "http://jsonresume.org/schema"
      ]
    },
    "awards": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [],
        "properties": {
          "awarder": {
            "type": "string",
            "description": "The name of the award given",
            "title": "awarder",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Time Magazine"
            ],
            "minLength": 1
          },
          "date": {
            "type": "string",
            "description": "Date of the award",
            "title": "date",
            "minLength": 10,
            "maxLength": 10,
            "format": "date"
          },
          "summary": {
            "type": "string",
            "description": "A brief summary of the award",
            "title": "summary",
            "examples": [
              "Received for my work with Quantum Physics"
            ],
            "minLength": 1,
            "pattern": "^[^\\s].*[^\\s]$"
          },
          "title": {
            "type": "string",
            "description": "Title of the award",
            "title": "title",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "One of the 100 greatest minds of the century"
            ],
            "minLength": 1
          }
        },
        "additionalProperties": false
      },
      "description": "Specify any awards you have received throughout your professional career",
      "title": "awards"
    },
    "basics": {
      "type": "object",
      "required": [
        "email",
        "label",
        "location",
        "name",
        "profiles",
        "summary"
      ],
      "properties": {
        "email": {
          "type": "string",
          "description": "an Email address string matching the pattern ^(?!\\.)(?!.*\\.\\.)([A-Z0-9_+-.]*)[A-Z0-9_+-]@([A-Z0-9][A-Z0-9-]*\\.)+[A-Z]{2,}$",
          "title": "Email",
          "examples": [
            "<local-part>@<domain>",
            "foo@bar.com",
            "foo.bar@baz.com"
          ],
          "format": "email",
          "pattern": "^(?!\\.)(?!.*\\.\\.)([A-Z0-9_+-.]*)[A-Z0-9_+-]@([A-Z0-9][A-Z0-9-]*\\.)+[A-Z]{2,}$"
        },
        "image": {
          "type": "string",
          "description": "URL to a image in JPEG or PNG format (as per RFC 3986)",
          "title": "image",
          "examples": [
            "https://example.com",
            "https://example.com/#section",
            "http://example.com:8080",
            "http://üçåüçåüçå.ws",
            "https://www.√ºbercool.de"
          ],
          "format": "uri"
        },
        "label": {
          "type": "string",
          "description": "Label",
          "title": "label",
          "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
          "examples": [
            "Web Developer"
          ],
          "minLength": 1
        },
        "location": {
          "type": "object",
          "required": [
            "city",
            "countryCode"
          ],
          "properties": {
            "address": {
              "type": "string",
              "description": "To add multiple address lines, use \"\\n\".",
              "title": "address",
              "examples": [
                "1234 Gl√ºcklichkeit Stra√üe\nHinterhaus 5. Etage li."
              ],
              "minLength": 1,
              "pattern": "^[^\\s].*[^\\s]$"
            },
            "city": {
              "type": "string",
              "description": "City",
              "title": "city",
              "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
              "examples": [
                "Berlin",
                "New York",
                "San Francisco"
              ],
              "minLength": 1
            },
            "countryCode": {
              "type": "string",
              "description": "Country code as per ISO-3166-1 ALPHA-2",
              "title": "countryCode",
              "examples": [
                "US",
                "AU",
                "IN"
              ],
              "minLength": 2,
              "maxLength": 2,
              "pattern": "^[A-Z]{2}$"
            },
            "postalCode": {
              "type": "string",
              "description": "European postal code",
              "title": "postalCode",
              "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
              "examples": [
                "12209"
              ],
              "minLength": 1
            },
            "region": {
              "type": "string",
              "description": "The general region where you live. Can be a US state, or a province",
              "title": "region",
              "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
              "examples": [
                "California",
                "Quebec"
              ],
              "minLength": 1
            }
          },
          "additionalProperties": false
        },
        "name": {
          "type": "string",
          "description": "Your full name",
          "title": "name",
          "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
          "examples": [
            "Thomas Anderson"
          ],
          "minLength": 1
        },
        "phone": {
          "type": "string",
          "description": "Phone numbers are stored as strings so use any format you like, as long as it is a valid phone number. E.164 format is recommended.",
          "title": "phone",
          "examples": [
            "712-117-2923"
          ],
          "pattern": "^\\+[1-9]\\d{1,14}$"
        },
        "profiles": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "network",
              "url",
              "username"
            ],
            "properties": {
              "network": {
                "type": "string",
                "description": "The name of the social network",
                "title": "network",
                "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
                "examples": [
                  "Facebook",
                  "Twitter"
                ],
                "minLength": 1
              },
              "url": {
                "type": "string",
                "description": "The URL of the profile on the social network",
                "title": "url",
                "examples": [
                  "http://twitter.example.com/neutralthoughts"
                ],
                "format": "uri"
              },
              "username": {
                "type": "string",
                "description": "The username of the profile on the social network",
                "title": "username",
                "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
                "examples": [
                  "neutralthoughts"
                ],
                "minLength": 1
              }
            },
            "additionalProperties": false
          },
          "description": "Specify any number of social networks that you participate in",
          "title": "profiles"
        },
        "summary": {
          "type": "string",
          "description": "Write a short 2-3 sentence biography about yourself",
          "title": "summary",
          "examples": [
            "Web Developer with a passion for web-based applications"
          ],
          "minLength": 1,
          "pattern": "^[^\\s].*[^\\s]$"
        },
        "url": {
          "type": "string",
          "description": "URL (as per RFC 3986) to your website, e.g. personal homepage",
          "title": "url",
          "examples": [
            "http://thomasanderson.com"
          ],
          "format": "uri"
        }
      },
      "additionalProperties": false
    },
    "certificates": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [],
        "properties": {
          "name": {
            "type": "string",
            "description": "name of the certificate",
            "title": "Name",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "e.g. Certified Kubernetes Administrator"
            ],
            "minLength": 1
          },
          "date": {
            "type": "string",
            "description": "a string that is a valid YYYY-MM-DD date",
            "title": "date",
            "minLength": 10,
            "maxLength": 10,
            "format": "date"
          },
          "url": {
            "type": "string",
            "description": "the url of the certificate",
            "title": "url",
            "examples": [
              "http://example.com/cert.pdf"
            ],
            "format": "uri"
          },
          "issuer": {
            "type": "string",
            "description": "issuer of the certificate",
            "title": "issuer",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "e.g. CNCF"
            ],
            "minLength": 1
          }
        },
        "additionalProperties": false
      },
      "description": "Specify any certificates you have received throughout your professional career",
      "title": "certificates"
    },
    "education": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "area",
          "institution",
          "startDate",
          "studyType"
        ],
        "properties": {
          "area": {
            "type": "string",
            "description": "e.g. Arts",
            "title": "area",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Arts",
              "Computer Science"
            ],
            "minLength": 1
          },
          "courses": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "a non empty string",
              "title": "course",
              "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
              "examples": [
                "H1302 - Introduction to American history"
              ],
              "minLength": 1
            },
            "description": "List notable courses/subjects",
            "title": "courses",
            "examples": [
              [
                "course1",
                "course2"
              ]
            ]
          },
          "endDate": {
            "type": "string",
            "description": "end date of education",
            "title": "endDate",
            "examples": [
              "2020-01-01"
            ],
            "minLength": 10,
            "maxLength": 10,
            "format": "date"
          },
          "score": {
            "type": "string",
            "description": "grade point average, e.g. 3.67/4.0",
            "title": "score",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "3.67/4.0"
            ],
            "minLength": 1
          },
          "institution": {
            "type": "string",
            "description": "name of the institution",
            "title": "institution",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Massachusetts Institute of Technology"
            ],
            "minLength": 1
          },
          "location": {
            "type": "string",
            "description": "a non-empty string with no leading or trailing whitespace",
            "title": "TrimmedNonEmpty",
            "minLength": 1,
            "pattern": "^[^\\s].*[^\\s]$"
          },
          "startDate": {
            "type": "string",
            "description": "start date of education",
            "title": "startDate",
            "examples": [
              "1970-01-01T00:00"
            ],
            "minLength": 10,
            "maxLength": 10,
            "format": "date"
          },
          "studyType": {
            "type": "string",
            "description": "the type of study",
            "title": "studyType",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Bachelor",
              "Master",
              "Doctorate"
            ],
            "minLength": 1
          },
          "url": {
            "type": "string",
            "description": "URL (as per RFC 3986) of the institution",
            "title": "url",
            "examples": [
              "http://mit.com"
            ],
            "format": "uri"
          }
        },
        "additionalProperties": false
      }
    },
    "interests": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [],
        "properties": {
          "keywords": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "a non empty string",
              "title": "keyword",
              "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
              "examples": [
                "philosophy"
              ],
              "minLength": 1
            },
            "description": "List some keywords pertaining to this interest",
            "title": "keywords",
            "examples": [
              [
                "philosophy",
                "distributed systems"
              ]
            ]
          },
          "name": {
            "type": "string",
            "description": "Interest name",
            "title": "name",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Philosophy"
            ],
            "minLength": 1
          }
        },
        "additionalProperties": false
      }
    },
    "languages": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [],
        "properties": {
          "fluency": {
            "type": "string",
            "description": "e.g. Fluent, Beginner",
            "title": "fluency",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Fluent",
              "Beginner",
              "Intermediate",
              "Advanced",
              "Native"
            ],
            "minLength": 1
          },
          "language": {
            "type": "string",
            "description": "e.g. English, Spanish",
            "title": "language",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "English",
              "Spanish"
            ],
            "minLength": 1
          }
        },
        "additionalProperties": false
      },
      "description": "List any other languages you speak",
      "title": "languages"
    },
    "projects": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [],
        "properties": {
          "description": {
            "type": "string",
            "description": "Short summary of project",
            "title": "description",
            "examples": [
              "Collated works of 2017"
            ],
            "minLength": 1,
            "pattern": "^[^\\s].*[^\\s]$"
          },
          "endDate": {
            "type": "string",
            "description": "a string that is a valid YYYY-MM-DD date",
            "title": "StringDate",
            "minLength": 10,
            "maxLength": 10,
            "format": "date"
          },
          "entity": {
            "type": "string",
            "description": "Specify the relevant company/entity affiliations",
            "title": "entity",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Greenpeace",
              "Microsoft"
            ],
            "minLength": 1
          },
          "highlights": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "Specify multiple features",
              "title": "highlight",
              "examples": [
                "Directs you close but not quite there"
              ],
              "minLength": 1,
              "pattern": "^[^\\s].*[^\\s]$"
            },
            "description": "Specify multiple features",
            "title": "highlights"
          },
          "keywords": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "a non empty string",
              "title": "keyword",
              "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
              "examples": [
                "AngularJS",
                "elements"
              ],
              "minLength": 1
            },
            "description": "Specify special elements involved",
            "title": "keywords"
          },
          "name": {
            "type": "string",
            "description": "Name of the project",
            "title": "name",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "The World Wide Web"
            ],
            "minLength": 1
          },
          "roles": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "a non empty string",
              "title": "role",
              "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
              "examples": [
                "Team Lead",
                "Speaker",
                "Writer"
              ],
              "minLength": 1
            },
            "description": "Specify your role on this project or in company",
            "title": "roles"
          },
          "startDate": {
            "type": "string",
            "description": "a string that is a valid YYYY-MM-DD date",
            "title": "StringDate",
            "minLength": 10,
            "maxLength": 10,
            "format": "date"
          },
          "type": {
            "type": "string",
            "description": "Type of project",
            "title": "type",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "volunteering",
              "presentation",
              "talk",
              "application",
              "conference"
            ],
            "minLength": 1
          },
          "url": {
            "type": "string",
            "description": "URL (as per RFC 3986) to the project page",
            "title": "url",
            "examples": [
              "http://www.computer.org.csdl/mags/co/1996/10/rx069-abs.html"
            ],
            "format": "uri"
          }
        },
        "additionalProperties": false
      },
      "description": "Specify career projects",
      "title": "projects"
    },
    "publications": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [],
        "properties": {
          "name": {
            "type": "string",
            "description": "The name of the publication",
            "title": "name",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "The World Wide Web"
            ],
            "minLength": 1
          },
          "publisher": {
            "type": "string",
            "description": "The publisher of the publication",
            "title": "publisher",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "IEEE",
              "Computer Magazine"
            ],
            "minLength": 1
          },
          "releaseDate": {
            "type": "string",
            "description": "a string that is a valid YYYY-MM-DD date",
            "title": "StringDate",
            "minLength": 10,
            "maxLength": 10,
            "format": "date"
          },
          "summary": {
            "type": "string",
            "description": "Short summary of publication",
            "title": "summary",
            "examples": [
              "Discussion of the World Wide Web, HTTP, HTML"
            ],
            "minLength": 1,
            "pattern": "^[^\\s].*[^\\s]$"
          },
          "url": {
            "type": "string",
            "description": "URL (as per RFC 3986) to the publication",
            "title": "url",
            "examples": [
              "http://www.computer.org.example.com/csdl/mags/co/1996/10/rx069-abs.html"
            ],
            "format": "uri"
          }
        },
        "additionalProperties": false
      },
      "description": "Specify your publications through your career",
      "title": "publications"
    },
    "references": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "The name of the reference",
            "title": "name",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Timothy Cook"
            ],
            "minLength": 1
          },
          "reference": {
            "type": "string",
            "description": "The reference text",
            "title": "reference",
            "examples": [
              "Joe blogs was a great employee, who turned up to work at least once a week. He exceeded my expectations when it came to doing nothing."
            ],
            "minLength": 1,
            "pattern": "^[^\\s].*[^\\s]$"
          }
        },
        "additionalProperties": false
      },
      "description": "List references you have received",
      "title": "references"
    },
    "skills": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "keywords",
          "name"
        ],
        "properties": {
          "keywords": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "a non empty string",
              "title": "keyword",
              "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
              "examples": [
                "Rust"
              ],
              "minLength": 1
            },
            "description": "List some keywords pertaining to this skill",
            "title": "keywords",
            "examples": [
              [
                "Rust",
                "Java"
              ]
            ]
          },
          "level": {
            "type": "string",
            "description": "Level of expertise",
            "title": "level",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Master",
              "Intermediate"
            ],
            "minLength": 1
          },
          "name": {
            "type": "string",
            "description": "Name of the skill",
            "title": "name",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Web Development"
            ],
            "minLength": 1
          }
        },
        "additionalProperties": false
      },
      "description": "List out your professional skill-set",
      "title": "skills"
    },
    "volunteer": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [],
        "properties": {
          "endDate": {
            "type": "string",
            "description": "a string that is a valid YYYY-MM-DD date",
            "title": "StringDate",
            "minLength": 10,
            "maxLength": 10,
            "format": "date"
          },
          "highlights": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "a non-empty string with no leading or trailing whitespace",
              "title": "highlight",
              "examples": [
                "Increased profits by 20% from 2011-2012 through viral advertising"
              ],
              "minLength": 1,
              "pattern": "^[^\\s].*[^\\s]$"
            },
            "description": "Specify accomplishments and achievements",
            "title": "highlights"
          },
          "organization": {
            "type": "string",
            "description": "Name of the organization",
            "title": "organization",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Facebook"
            ],
            "minLength": 1
          },
          "position": {
            "type": "string",
            "description": "The title of your position at the company",
            "title": "position",
            "pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
            "examples": [
              "Software Engineer"
            ],
            "minLength": 1
          },
          "startDate": {
            "type": "string",
            "description": "a string that is a valid YYYY-MM-DD date",
            "title": "StringDate",
            "minLength": 10,
            "maxLength": 10,
            "format": "date"
          },
          "summary": {
            "type": "string",
            "description": "Give an overview of your responsibilities at the company",
            "title": "summary",
            "examples": [
              "My day-to-day activities involved designing and building web applications..."
            ],
            "minLength": 1,
            "pattern": "^[^\\s].*[^\\s]$"
          },
          "url": {
            "type": "string",
            "description": "URL (as per RFC 3986) of the company",
            "title": "url",
            "examples": [
              "https://facebook.example.com"
            ],
            "format": "uri"
          }
        },
        "additionalProperties": false
      }
    },
    "work": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "description",
          "name",
          "roles"
        ],
        "properties": {
          "contact": {
            "type": "object",
            "required": [
              "name",
              "email"
            ],
            "properties": {
              "name": {
                "type": "string",

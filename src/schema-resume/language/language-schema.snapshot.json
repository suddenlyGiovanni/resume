{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [],
	"properties": {
		"fluency": {
			"type": "string",
			"description": "e.g. Fluent, Beginner",
			"title": "fluency",
			"pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
			"examples": [
				"Fluent",
				"Beginner",
				"Intermediate",
				"Advanced",
				"Native"
			],
			"minLength": 1
		},
		"language": {
			"type": "string",
			"description": "e.g. English, Spanish",
			"title": "language",
			"pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
			"examples": [
				"English",
				"Spanish"
			],
			"minLength": 1
		}
	},
	"additionalProperties": false
}
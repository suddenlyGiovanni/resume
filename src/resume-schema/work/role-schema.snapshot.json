{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"title",
		"startDate",
		"responsibilities"
	],
	"properties": {
		"title": {
			"type": "string",
			"description": "The title of your position at the company: <seniority level> <role title>",
			"title": "title",
			"examples": [
				"Junior Software Developer",
				"Senior Software Engineer"
			]
		},
		"startDate": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"endDate": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"responsibilities": {
			"type": "array",
			"items": {
				"type": "string",
				"description": "A specific responsibility",
				"title": "responsibility",
				"examples": [
					"code owner of x"
				]
			}
		},
		"highlights": {
			"type": "array",
			"minItems": 1,
			"items": [
				{
					"type": "string",
					"description": "A specific accomplishment",
					"title": "highlight",
					"examples": [
						"Increased profits by 20% from 2011-2012 through viral advertising"
					]
				}
			],
			"additionalItems": {
				"type": "string",
				"description": "A specific accomplishment",
				"title": "highlight",
				"examples": [
					"Increased profits by 20% from 2011-2012 through viral advertising"
				]
			}
		},
		"technologies": {
			"type": "array",
			"minItems": 1,
			"items": [
				{
					"type": "string",
					"description": "A specific technology used",
					"title": "technology",
					"examples": [
						"React",
						"Node.js"
					]
				}
			],
			"additionalItems": {
				"type": "string",
				"description": "A specific technology used",
				"title": "technology",
				"examples": [
					"React",
					"Node.js"
				]
			}
		}
	},
	"additionalProperties": false
}
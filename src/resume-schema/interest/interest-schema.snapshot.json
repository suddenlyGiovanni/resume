{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [],
	"properties": {
		"keywords": {
			"type": "array",
			"items": {
				"type": "string",
				"description": "a non-empty string with no leading or trailing whitespace",
				"title": "keyword",
				"examples": [
					"philosophy"
				],
				"minLength": 1,
				"pattern": "^[^\\s].*[^\\s]$"
			},
			"description": "List some keywords pertaining to this interest",
			"title": "keywords",
			"examples": [
				[
					"philosophy",
					"distributed systems"
				]
			]
		},
		"name": {
			"type": "string",
			"description": "Interest name",
			"title": "name",
			"examples": [
				"Philosophy"
			],
			"minLength": 1,
			"pattern": "^[^\\s].*[^\\s]$"
		}
	},
	"additionalProperties": false
}
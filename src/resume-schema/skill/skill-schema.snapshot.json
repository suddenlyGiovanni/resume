{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"name",
		"keywords"
	],
	"properties": {
		"level": {
			"type": "string",
			"description": "Level of expertise",
			"title": "level",
			"examples": [
				"Master",
				"Intermediate"
			]
		},
		"name": {
			"type": "string",
			"description": "Name of the skill",
			"title": "name",
			"examples": [
				"Web Development"
			]
		},
		"keywords": {
			"type": "array",
			"items": {
				"type": "string",
				"description": "a string",
				"title": "keyword",
				"examples": [
					"Rust"
				]
			}
		}
	},
	"additionalProperties": false
}
{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"email",
		"label",
		"name",
		"summary",
		"location",
		"profiles"
	],
	"properties": {
		"email": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"image": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"label": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"name": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"phone": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"summary": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"url": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"location": {
			"type": "object",
			"required": [
				"city",
				"countryCode"
			],
			"properties": {
				"address": {
					"type": "string",
					"description": "To add multiple address lines, use \"\\n\".",
					"title": "address",
					"examples": [
						"1234 Glücklichkeit Straße\nHinterhaus 5. Etage li."
					]
				},
				"city": {
					"type": "string",
					"description": "City",
					"title": "city",
					"examples": [
						"Berlin",
						"New York",
						"San Francisco"
					]
				},
				"countryCode": {
					"type": "string",
					"description": "a string",
					"title": "string"
				},
				"postalCode": {
					"type": "string",
					"description": "European postal code",
					"title": "postalCode",
					"examples": [
						"12209"
					]
				},
				"region": {
					"type": "string",
					"description": "The general region where you live. Can be a US state, or a province",
					"title": "region",
					"examples": [
						"California",
						"Quebec"
					]
				}
			},
			"additionalProperties": false
		},
		"profiles": {
			"type": "array",
			"items": {
				"type": "object",
				"required": [
					"network",
					"url",
					"username"
				],
				"properties": {
					"network": {
						"type": "string",
						"description": "The name of the social network",
						"title": "network",
						"examples": [
							"Facebook",
							"Twitter"
						]
					},
					"url": {
						"type": "string",
						"description": "a string",
						"title": "string"
					},
					"username": {
						"type": "string",
						"description": "The username of the profile on the social network",
						"title": "username",
						"examples": [
							"neutralthoughts"
						]
					}
				},
				"additionalProperties": false
			}
		}
	},
	"additionalProperties": false
}
{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"email",
		"label",
		"name",
		"summary",
		"location",
		"profiles"
	],
	"properties": {
		"email": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"image": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"label": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"name": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"phone": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"summary": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"url": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"location": {
			"type": "object",
			"required": [
				"city",
				"countryCode"
			],
			"properties": {
				"address": {
					"type": "string",
					"description": "a string",
					"title": "string"
				},
				"city": {
					"type": "string",
					"description": "a string",
					"title": "string"
				},
				"countryCode": {
					"type": "string",
					"description": "a string",
					"title": "string"
				},
				"postalCode": {
					"type": "string",
					"description": "a string",
					"title": "string"
				},
				"region": {
					"type": "string",
					"description": "a string",
					"title": "string"
				}
			},
			"additionalProperties": false
		},
		"profiles": {
			"type": "array",
			"items": {
				"type": "object",
				"required": [
					"network",
					"url",
					"username"
				],
				"properties": {
					"network": {
						"type": "string",
						"description": "The name of the social network",
						"title": "network",
						"examples": [
							"Facebook",
							"Twitter"
						]
					},
					"url": {
						"type": "string",
						"description": "a string",
						"title": "string"
					},
					"username": {
						"type": "string",
						"description": "The username of the profile on the social network",
						"title": "username",
						"examples": [
							"neutralthoughts"
						]
					}
				},
				"additionalProperties": false
			}
		}
	},
	"additionalProperties": false
}
{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [],
	"properties": {
		"name": {
			"type": "string",
			"description": "The name of the publication",
			"title": "name",
			"examples": [
				"The World Wide Web"
			]
		},
		"publisher": {
			"type": "string",
			"description": "The publisher of the publication",
			"title": "publisher",
			"examples": [
				"IEEE",
				"Computer Magazine"
			]
		},
		"releaseDate": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"summary": {
			"type": "string",
			"description": "Short summary of publication",
			"title": "summary",
			"examples": [
				"Discussion of the World Wide Web, HTTP, HTML"
			]
		},
		"url": {
			"$ref": "#/$defs/UrlString"
		}
	},
	"additionalProperties": false,
	"$defs": {
		"UrlString": {
			"type": "string",
			"description": "URL string that fulfills the URL requirements (as per RFC 3986)",
			"title": "url",
			"examples": [
				"https://example.com",

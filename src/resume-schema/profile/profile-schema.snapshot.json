{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"network",
		"url",
		"username"
	],
	"properties": {
		"network": {
			"type": "string",
			"description": "The name of the social network",
			"title": "network",
			"examples": [
				"Facebook",
				"Twitter"
			]
		},
		"url": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"username": {
			"type": "string",
			"description": "The username of the profile on the social network",
			"title": "username",
			"examples": [
				"neutralthoughts"
			]
		}
	},
	"additionalProperties": false
}
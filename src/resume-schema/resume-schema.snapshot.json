                "technologies": {
                  "type": "array",
                  "minItems": 1,
                  "items": [
                    {
                      "type": "string",
                      "description": "A specific technology used",
                      "title": "technology",
                      "examples": [
                        "React",
                        "Node.js"
                      ]
                    }
                  ],
                  "additionalItems": {

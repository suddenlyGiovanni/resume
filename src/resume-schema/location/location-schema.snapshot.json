{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"city",
		"countryCode"
	],
	"properties": {
		"address": {
			"type": "string",
			"description": "To add multiple address lines, use \"\\n\".",
			"title": "address",
			"examples": [
				"1234 Glücklichkeit Straße\nHinterhaus 5. Etage li."
			]
		},
		"city": {
			"type": "string",
			"description": "City",
			"title": "city",
			"examples": [
				"Berlin",
				"New York",
				"San Francisco"
			]
		},
		"countryCode": {
			"type": "string",
			"description": "a string",
			"title": "string"
		},
		"postalCode": {
			"type": "string",
			"description": "European postal code",
			"title": "postalCode",
			"examples": [
				"12209"
			]
		},
		"region": {
			"type": "string",
			"description": "The general region where you live. Can be a US state, or a province",
			"title": "region",
			"examples": [
				"California",
				"Quebec"
			]
		}
	},
	"additionalProperties": false
}
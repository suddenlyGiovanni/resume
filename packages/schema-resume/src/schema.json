{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$defs": {
		"Resume": {
			"type": "object",
			"required": ["$schema", "basics", "education", "skills", "work"],
			"properties": {
				"$schema": {
					"type": "string",
					"description": "link to the version of the schema that can validate the resume",
					"title": "$schema",
					"examples": ["http://jsonresume.org/schema"]
				},
				"awards": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Award"
					},
					"description": "Specify any awards you have received throughout your professional career",
					"title": "awards"
				},
				"basics": {
					"$ref": "#/$defs/Basics"
				},
				"certificates": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Certificate"
					},
					"description": "Specify any certificates you have received throughout your professional career",
					"title": "certificates"
				},
				"education": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Education"
					}
				},
				"interests": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Interest"
					}
				},
				"languages": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Language"
					},
					"description": "List any other languages you speak",
					"title": "languages"
				},
				"projects": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Project"
					},
					"description": "Specify career projects",
					"title": "projects"
				},
				"publications": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Publication"
					},
					"description": "Specify your publications through your career",
					"title": "publications"
				},
				"references": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Reference"
					},
					"description": "List references you have received",
					"title": "references"
				},
				"skills": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Skill"
					},
					"description": "List out your professional skill-set",
					"title": "skills"
				},
				"volunteer": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Volunteer"
					}
				},
				"work": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Work"
					}
				}
			},
			"additionalProperties": false
		},
		"Award": {
			"type": "object",
			"required": [],
			"properties": {
				"awarder": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "The name of the award given",
					"title": "awarder",
					"examples": ["Time Magazine"]
				},
				"date": {
					"$ref": "#/$defs/StringDate",
					"description": "Date of the award",
					"title": "date"
				},
				"summary": {
					"$ref": "#/$defs/TrimmedNonEmpty",
					"description": "A brief summary of the award",
					"title": "summary",
					"examples": ["Received for my work with Quantum Physics"]
				},
				"title": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Title of the award",
					"title": "title",
					"examples": ["One of the 100 greatest minds of the century"]
				}
			},
			"additionalProperties": false
		},
		"NonEmptyTrimmedString": {
			"type": "string",
			"description": "a non empty string",
			"title": "nonEmptyString",
			"pattern": "^\\S[\\s\\S]*\\S$|^\\S$|^$",
			"minLength": 1
		},
		"StringDate": {
			"type": "string",
			"description": "a string that is a valid YYYY-MM-DD date",
			"title": "StringDate",
			"minLength": 10,
			"maxLength": 10,
			"format": "date"
		},
		"TrimmedNonEmpty": {
			"type": "string",
			"description": "a non-empty string with no leading or trailing whitespace",
			"title": "TrimmedNonEmpty",
			"minLength": 1,
			"pattern": "^[^\\s].*[^\\s]$"
		},
		"Basics": {
			"type": "object",
			"required": ["email", "label", "location", "name", "profiles", "summary"],
			"properties": {
				"email": {
					"$ref": "#/$defs/Email"
				},
				"image": {
					"$ref": "#/$defs/UrlString",
					"description": "URL to a image in JPEG or PNG format (as per RFC 3986)",
					"title": "image"
				},
				"label": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Label",
					"title": "label",
					"examples": ["Web Developer"]
				},
				"location": {
					"$ref": "#/$defs/Location"
				},
				"name": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Your full name",
					"title": "name",
					"examples": ["Thomas Anderson"]
				},
				"phone": {
					"$ref": "#/$defs/PhoneString",
					"description": "Phone numbers are stored as strings so use any format you like, as long as it is a valid phone number. E.164 format is recommended.",
					"title": "phone",
					"examples": ["712-117-2923"]
				},
				"profiles": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/Profile"
					},
					"description": "Specify any number of social networks that you participate in",
					"title": "profiles"
				},
				"summary": {
					"$ref": "#/$defs/TrimmedNonEmpty",
					"description": "Write a short 2-3 sentence biography about yourself",
					"title": "summary",
					"examples": ["Web Developer with a passion for web-based applications"]
				},
				"url": {
					"$ref": "#/$defs/UrlString",
					"description": "URL (as per RFC 3986) to your website, e.g. personal homepage",
					"title": "url",
					"examples": ["http://thomasanderson.com"]
				}
			},
			"additionalProperties": false
		},
		"Email": {
			"type": "string",
			"description": "an Email address string matching the pattern ^(?!\\.)(?!.*\\.\\.)([A-Z0-9_+-.]*)[A-Z0-9_+-]@([A-Z0-9][A-Z0-9-]*\\.)+[A-Z]{2,}$",
			"title": "Email",
			"examples": ["<local-part>@<domain>", "foo@bar.com", "foo.bar@baz.com"],
			"format": "email",
			"pattern": "^(?!\\.)(?!.*\\.\\.)([A-Z0-9_+-.]*)[A-Z0-9_+-]@([A-Z0-9][A-Z0-9-]*\\.)+[A-Z]{2,}$"
		},
		"UrlString": {
			"type": "string",
			"description": "a string that fulfills the URL requirements (as per RFC 3986)",
			"title": "UrlString",
			"examples": [
				"https://example.com",
				"https://example.com/#section",
				"http://example.com:8080",
				"http://üçåüçåüçå.ws",
				"https://www.√ºbercool.de"
			],
			"format": "uri"
		},
		"Location": {
			"type": "object",
			"required": ["city", "countryCode"],
			"properties": {
				"address": {
					"$ref": "#/$defs/TrimmedNonEmpty",
					"description": "To add multiple address lines, use \"\\n\".",
					"title": "address",
					"examples": ["1234 Gl√ºcklichkeit Stra√üe\nHinterhaus 5. Etage li."]
				},
				"city": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "City",
					"title": "city",
					"examples": ["Berlin", "New York", "San Francisco"]
				},
				"countryCode": {
					"$ref": "#/$defs/countryCode"
				},
				"postalCode": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "European postal code",
					"title": "postalCode",
					"examples": ["12209"]
				},
				"region": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "The general region where you live. Can be a US state, or a province",
					"title": "region",
					"examples": ["California", "Quebec"]
				}
			},
			"additionalProperties": false
		},
		"countryCode": {
			"type": "string",
			"description": "Country code as per ISO-3166-1 ALPHA-2",
			"title": "countryCode",
			"examples": ["US", "AU", "IN"],
			"minLength": 2,
			"maxLength": 2,
			"pattern": "^[A-Z]{2}$"
		},
		"PhoneString": {
			"type": "string",
			"description": "a phone number conforming to the E.164 format standard",
			"title": "PhoneString",
			"examples": [
				"+4907121172923",
				"+441632960961",
				"+353861234567",
				"00353861234567",
				"+39 02 1234567",
				"+1-800-123-4567",
				"+1 800 123 4567",
				"+49 (0) 216 554 1036"
			],
			"pattern": "^\\+[1-9]\\d{1,14}$"
		},
		"Profile": {
			"type": "object",
			"required": ["network", "url", "username"],
			"properties": {
				"network": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "The name of the social network",
					"title": "network",
					"examples": ["Facebook", "Twitter"]
				},
				"url": {
					"$ref": "#/$defs/UrlString",
					"description": "The URL of the profile on the social network",
					"title": "url",
					"examples": ["http://twitter.example.com/neutralthoughts"]
				},
				"username": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "The username of the profile on the social network",
					"title": "username",
					"examples": ["neutralthoughts"]
				}
			},
			"additionalProperties": false
		},
		"Certificate": {
			"type": "object",
			"required": [],
			"properties": {
				"name": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "name of the certificate",
					"title": "Name",
					"examples": ["e.g. Certified Kubernetes Administrator"]
				},
				"date": {
					"$ref": "#/$defs/StringDate",
					"title": "date"
				},
				"url": {
					"$ref": "#/$defs/UrlString",
					"description": "the url of the certificate",
					"title": "url",
					"examples": ["http://example.com/cert.pdf"]
				},
				"issuer": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "issuer of the certificate",
					"title": "issuer",
					"examples": ["e.g. CNCF"]
				}
			},
			"additionalProperties": false
		},
		"Education": {
			"type": "object",
			"required": ["area", "institution", "startDate", "studyType"],
			"properties": {
				"area": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "e.g. Arts",
					"title": "area",
					"examples": ["Arts", "Computer Science"]
				},
				"courses": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/NonEmptyTrimmedString"
					},
					"description": "List notable courses/subjects",
					"title": "courses",
					"examples": [["CS50", "H1302 - Introduction to American history"]]
				},
				"endDate": {
					"$ref": "#/$defs/StringDate",
					"description": "end date of education",
					"title": "endDate",
					"examples": ["2020-01-01"]
				},
				"score": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "grade point average, e.g. 3.67/4.0",
					"title": "score",
					"examples": ["3.67/4.0"]
				},
				"institution": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "name of the institution",
					"title": "institution",
					"examples": ["Massachusetts Institute of Technology"]
				},
				"location": {
					"$ref": "#/$defs/TrimmedNonEmpty"
				},
				"startDate": {
					"$ref": "#/$defs/StringDate",
					"description": "start date of education",
					"title": "startDate",
					"examples": ["1970-01-01T00:00"]
				},
				"studyType": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "the type of study",
					"title": "studyType",
					"examples": ["Bachelor", "Master", "Doctorate"]
				},
				"url": {
					"$ref": "#/$defs/UrlString",
					"description": "URL (as per RFC 3986) of the institution",
					"title": "url",
					"examples": ["http://mit.com"]
				}
			},
			"additionalProperties": false
		},
		"Interest": {
			"type": "object",
			"required": [],
			"properties": {
				"keywords": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/NonEmptyTrimmedString"
					},
					"description": "List some keywords pertaining to this interest",
					"title": "keywords",
					"examples": [["philosophy", "distributed systems"]]
				},
				"name": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Interest name",
					"title": "name",
					"examples": ["Philosophy"]
				}
			},
			"additionalProperties": false
		},
		"Language": {
			"type": "object",
			"required": [],
			"properties": {
				"fluency": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "e.g. Fluent, Beginner",
					"title": "fluency",
					"examples": ["Fluent", "Beginner", "Intermediate", "Advanced", "Native"]
				},
				"language": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "e.g. English, Spanish",
					"title": "language",
					"examples": ["English", "Spanish"]
				}
			},
			"additionalProperties": false
		},
		"Project": {
			"type": "object",
			"required": [],
			"properties": {
				"description": {
					"$ref": "#/$defs/TrimmedNonEmpty",
					"description": "Short summary of project",
					"title": "description",
					"examples": ["Collated works of 2017"]
				},
				"endDate": {
					"$ref": "#/$defs/StringDate"
				},
				"entity": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Specify the relevant company/entity affiliations",
					"title": "entity",
					"examples": ["Greenpeace", "Microsoft"]
				},
				"highlights": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/TrimmedNonEmpty"
					},
					"description": "Specify multiple features",
					"title": "highlights",
					"examples": [["Directs you close but not quite there"]]
				},
				"keywords": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/NonEmptyTrimmedString"
					},
					"description": "Specify special elements involved",
					"title": "keywords",
					"examples": [["AngularJS", "elements"]]
				},
				"name": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Name of the project",
					"title": "name",
					"examples": ["The World Wide Web"]
				},
				"roles": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/NonEmptyTrimmedString"
					},
					"description": "Specify your role on this project or in company",
					"title": "roles",
					"examples": [["Team Lead", "Speaker", "Writer"]]
				},
				"startDate": {
					"$ref": "#/$defs/StringDate"
				},
				"type": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Type of project",
					"title": "type",
					"examples": ["volunteering", "presentation", "talk", "application", "conference"]
				},
				"url": {
					"$ref": "#/$defs/UrlString",
					"description": "URL (as per RFC 3986) to the project page",
					"title": "url",
					"examples": ["http://www.computer.org.csdl/mags/co/1996/10/rx069-abs.html"]
				}
			},
			"additionalProperties": false
		},
		"Publication": {
			"type": "object",
			"required": [],
			"properties": {
				"name": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "The name of the publication",
					"title": "name",
					"examples": ["The World Wide Web"]
				},
				"publisher": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "The publisher of the publication",
					"title": "publisher",
					"examples": ["IEEE", "Computer Magazine"]
				},
				"releaseDate": {
					"$ref": "#/$defs/StringDate"
				},
				"summary": {
					"$ref": "#/$defs/TrimmedNonEmpty",
					"description": "Short summary of publication",
					"title": "summary",
					"examples": ["Discussion of the World Wide Web, HTTP, HTML"]
				},
				"url": {
					"$ref": "#/$defs/UrlString",
					"description": "URL (as per RFC 3986) to the publication",
					"title": "url",
					"examples": ["http://www.computer.org.example.com/csdl/mags/co/1996/10/rx069-abs.html"]
				}
			},
			"additionalProperties": false
		},
		"Reference": {
			"type": "object",
			"required": ["name"],
			"properties": {
				"name": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "The name of the reference",
					"title": "name",
					"examples": ["Timothy Cook"]
				},
				"reference": {
					"$ref": "#/$defs/TrimmedNonEmpty",
					"description": "The reference text",
					"title": "reference",
					"examples": [
						"Joe blogs was a great employee, who turned up to work at least once a week. He exceeded my expectations when it came to doing nothing."
					]
				}
			},
			"additionalProperties": false
		},
		"Skill": {
			"type": "object",
			"required": ["keywords", "name"],
			"properties": {
				"keywords": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/NonEmptyTrimmedString"
					},
					"description": "List some keywords pertaining to this skill",
					"title": "keywords",
					"examples": [["Rust", "Java"]]
				},
				"level": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Level of expertise",
					"title": "level",
					"examples": ["Master", "Intermediate"]
				},
				"name": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Name of the skill",
					"title": "name",
					"examples": ["Web Development"]
				}
			},
			"additionalProperties": false
		},
		"Volunteer": {
			"type": "object",
			"required": [],
			"properties": {
				"endDate": {
					"$ref": "#/$defs/StringDate"
				},
				"highlights": {
					"type": "array",
					"items": {
						"$ref": "#/$defs/TrimmedNonEmpty"
					},
					"description": "Specify accomplishments and achievements",
					"title": "highlights",
					"examples": [["Increased profits by 20% from 2011-2012 through viral advertising"]]
				},
				"organization": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Name of the organization",
					"title": "organization",
					"examples": ["Facebook"]
				},
				"position": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "The title of your position at the company",
					"title": "position",
					"examples": ["Software Engineer"]
				},
				"startDate": {
					"$ref": "#/$defs/StringDate"
				},
				"summary": {
					"$ref": "#/$defs/TrimmedNonEmpty",
					"description": "Give an overview of your responsibilities at the company",
					"title": "summary",
					"examples": [
						"My day-to-day activities involved designing and building web applications..."
					]
				},
				"url": {
					"$ref": "#/$defs/UrlString",
					"description": "URL (as per RFC 3986) of the company",
					"title": "url",
					"examples": ["https://facebook.example.com"]
				}
			},
			"additionalProperties": false
		},
		"Work": {
			"type": "object",
			"required": ["description", "name", "roles"],
			"properties": {
				"contact": {
					"type": "object",
					"required": ["name", "email"],
					"properties": {
						"name": {
							"$ref": "#/$defs/NonEmptyTrimmedString",
							"description": "The name and role of the contact person",
							"title": "name",
							"examples": ["Mark Zuckerberg (CTO)"]
						},
						"email": {
							"$ref": "#/$defs/Email"
						},
						"phone": {
							"$ref": "#/$defs/PhoneString"
						}
					},
					"additionalProperties": false
				},
				"description": {
					"$ref": "#/$defs/TrimmedNonEmpty",
					"description": "A short description of the company",
					"title": "description",
					"examples": ["Social Media Company", "Educational Software Company"]
				},
				"location": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Location of the company",
					"title": "location",
					"examples": ["Menlo Park, CA"]
				},
				"name": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "Name of the company",
					"title": "name",
					"examples": ["Facebook"]
				},
				"roles": {
					"type": "array",
					"minItems": 1,
					"items": {
						"$ref": "#/$defs/Role"
					},
					"description": "The roles you had at the company, in reverse chronological order",
					"title": "roles"
				},
				"summary": {
					"$ref": "#/$defs/TrimmedNonEmpty",
					"description": "A brief introduction of what the company does; a tagline, a mission statement, an elevator pitch; something that gives a sense of the company in a few words",
					"title": "summary",
					"examples": [
						"An educational software company that helps students learn through interactive games",
						"Help users to handle all their administrative tasks digitally with ease."
					]
				},
				"url": {
					"$ref": "#/$defs/UrlString"
				}
			},
			"additionalProperties": false
		},
		"Role": {
			"type": "object",
			"required": ["title", "startDate", "responsibilities"],
			"properties": {
				"title": {
					"$ref": "#/$defs/NonEmptyTrimmedString",
					"description": "The title of your position at the company: <seniority level> <role title>",
					"title": "title",
					"examples": ["Junior Software Developer", "Senior Software Engineer"]
				},
				"startDate": {
					"$ref": "#/$defs/StringDate",
					"description": "The date when you started working in this role",
					"title": "startDate",
					"examples": ["2011-01-01"]
				},
